<h1>New Party</h1>
<div class="new">
  <div class="container">
    <div class="row">
      <%= form_for @party do |f| %>
      <div class="one column">
        <%= f.label :name %>
      </div>
      <div class="three columns">
        <%= f.text_field :name %>
      </div>
      <div class="two columns">
        <%= f.label :table_number %>
      </div>
      <div class="two columns">
        <%= f.text_field :table_number %>
      </div>
      <div class="one column">
        <%= f.submit %>
      </div>
    </div>
    <div class="row">
      <div class= "one column">
      </div>
      <div class="three columns"
          <a><%= link_to "Back to Parties", server_path %></a>
      </div>
    </div>
  </div>
  </div>

<% end %>
